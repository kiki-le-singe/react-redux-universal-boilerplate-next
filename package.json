  },
  "scripts": {
    "build:client": "better-npm-run build:client",
    "deploy": "better-npm-run deploy",
    "prod": "better-npm-run prod",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "start:server": "better-npm-run start:server",
    "start:client:server": "better-npm-run start:client:server",
    "lint": "eslint src tests --ext .jsx,.js"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all --parallel start:client:server start:server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "npm run dev",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:client:server": {
      "command": "babel-node -- webpack/dev.server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:server": {
      "command": "nodemon ./node_modules/.bin/babel-node -- src/server/index",
      "env": {
        "NODE_ENV": "development",
        "NODE_PATH": "src",
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "babel-node src/server/index",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run build:client && npm run prod"
    },
    "build:client": {
      "command": "./node_modules/.bin/babel-node node_modules/.bin/webpack --progress --config webpack/prod.config",
      "env": {
        "NODE_ENV": "production",
        "NODE_PATH": "src",
        "DEBUG": "app:*"
      }
    }
  }
}
